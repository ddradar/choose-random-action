{
  "name": "choose-random-action",
  "description": "Choose one randomly from multiple user inputs",
  "version": "1.0.4",
  "author": "ddradar",
  "private": true,
  "main": "dist/index.js",
  "repository": "https://github.com/ddradar/choose-random-action",
  "keywords": [
    "actions",
    "github-actions",
    "random"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=12",
    "yarn": "^1.22.4"
  },
  "scripts": {
    "build": "ncc build src/main.ts -m --license licenses.txt",
    "fix": "run-p \"lint:eslint --fix\" \"lint:prettier --write\"",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint --ext .ts --ignore-path .gitignore .",
    "lint:prettier": "prettier --ignore-path .gitignore -l \"**/*.{ts,json,yml}\"",
    "prepare": "husky install",
    "test": "jest"
  },
  "lint-staged": {
    "*.{ts}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,yml}": "prettier --write"
  },
  "dependencies": {
    "@actions/core": "^1.2.6"
  },
  "devDependencies": {
    "@types/jest": "^26.0.21",
    "@types/js-yaml": "^4.0.0",
    "@types/node": "^12.20.6",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.20.0",
    "@vercel/ncc": "^0.27.0",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.2",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": "^6.0.0",
    "jest": "^27.0.0-next.5",
    "js-yaml": "^4.0.0",
    "lint-staged": "^10.5.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "ts-jest": "^27.0.0-next.9",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  }
}
