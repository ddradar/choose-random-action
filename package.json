{
  "name": "choose-random-action",
  "description": "Choose one randomly from multiple user inputs",
  "version": "1.0.8",
  "author": "ddradar",
  "private": true,
  "main": "dist/index.js",
  "repository": "https://github.com/ddradar/choose-random-action",
  "keywords": [
    "actions",
    "github-actions",
    "random"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=12",
    "yarn": "^1.22.4"
  },
  "scripts": {
    "build": "ncc build src/main.ts -m --license licenses.txt",
    "fix": "run-p \"lint:eslint --fix\" \"lint:prettier --write\"",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint --ext .ts --ignore-path .gitignore .",
    "lint:prettier": "prettier --ignore-path .gitignore -l \"**/*.{ts,json,yml}\"",
    "prepare": "husky install",
    "test": "jest"
  },
  "lint-staged": {
    "*.{ts}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,yml}": "prettier --write"
  },
  "dependencies": {
    "@actions/core": "^1.5.0"
  },
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.2",
    "@vercel/ncc": "^0.31.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^24.4.2",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": "^7.0.2",
    "jest": "^27.2.1",
    "lint-staged": "^11.1.2",
    "npm-run-all2": "^5.0.2",
    "prettier": "^2.4.1",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3"
  },
  "resolutions": {
    "chalk": "^4.0.0",
    "cssom": "^0.4.4",
    "eslint-visitor-keys": "^2.0.0",
    "js-yaml": "^4.1.0",
    "optionator": "^0.9.1",
    "supports-color": "^8.0.0",
    "type-fest": "^1.2.0"
  }
}
