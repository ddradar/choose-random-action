{
  "name": "choose-random-action",
  "version": "0.0.1",
  "private": true,
  "description": "Choose one randomly from multiple user inputs",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "npm run format-check -- --write",
    "format-check": "prettier --ignore-path .gitignore -l \"**/*.{json,yaml,yml}\"",
    "lint": "eslint --ext .ts,.js --ignore-path .gitignore .",
    "pack": "ncc build",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run pack && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ddradar/choose-random-action.git"
  },
  "keywords": [
    "actions",
    "github-actions"
  ],
  "author": "ddradar",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.3"
  },
  "devDependencies": {
    "@types/jest": "^25.1.4",
    "@types/node": "^13.9.1",
    "@typescript-eslint/eslint-plugin": "^2.24.0",
    "@typescript-eslint/parser": "^2.24.0",
    "@zeit/ncc": "^0.21.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-simple-import-sort": "^5.0.2",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "lint-staged": "^10.0.8",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "ts-jest": "^25.2.1",
    "typescript": "^3.8.3"
  }
}
