{
  "name": "choose-random-action",
  "description": "Choose one randomly from multiple user inputs",
  "version": "1.0.9",
  "author": "ddradar",
  "private": true,
  "main": "dist/index.js",
  "repository": "https://github.com/ddradar/choose-random-action",
  "keywords": [
    "actions",
    "github-actions",
    "random"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "build": "ncc build src/main.ts -m --license licenses.txt",
    "fix": "run-p \"lint:eslint --fix\" \"lint:prettier --write\"",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint --ext .ts --ignore-path .gitignore .",
    "lint:prettier": "prettier --ignore-path .gitignore -l \"**/*.{ts,json,yml}\"",
    "prepare": "husky install",
    "test": "jest"
  },
  "lint-staged": {
    "*.{ts}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,yml}": "prettier --write"
  },
  "dependencies": {
    "@actions/core": "^1.6.0"
  },
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "@vercel/ncc": "^0.33.1",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^25.3.3",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": "^7.0.4",
    "jest": "^27.4.5",
    "lint-staged": "^12.1.4",
    "npm-run-all2": "^5.0.2",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  }
}
